{
    "bimserver": {
        "controller": {
            "apiVersion": "v1",
            "kind": "ReplicationController",
            "metadata": {
                "labels": {
                    "name": "bimserver"
                },
                "name": "bimserver-controller"
            },
            "spec": {
                "replicas": 1,
                "selector": {
                    "app": "bimserver"
                },
                "template": {
                    "metadata": {
                        "name": "bimserver",
                        "labels": {
                            "app": "bimserver"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "name": "bimserver",
                                "image": "jenca/docker-bimserver:1.4.0-FINAL-2015-11-04",
                                "env": [
                                    {
                                        "name": "TOMCAT_USER",
                                        "value": "XXX"
                                    },
                                    {
                                        "name": "TOMCAT_PASSWORD",
                                        "value": "XXX"
                                    }
                                ],
                                "ports": [
                                    {
                                        "containerPort": 8080
                                    }
                                ]
                            }
                        ]
                    }
                }
            }
        },
        "description": "This is the description for the bimserver",
        "service": {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "name": "bimserver",
                "labels": {
                    "app": "bimserver"
                }
            },
            "spec": {
                "type": "NodePort",
                "ports": [
                    {
                        "port": 8080,
                        "targetPort": 8080,
                        "protocol": "TCP"
                    }
                ],
                "selector": {
                    "app": "bimserver"
                }
            }
        },
        "id": "fa6248f0-eaba-11e5-aa74-5d34fea124fc",
        "name": "bimserver"
    },
    "duraark-system": {
        "controller": {
            "apiVersion": "v1",
            "kind": "ReplicationController",
            "metadata": {
                "labels": {
                    "name": "duraark"
                },
                "name": "duraark-controller"
            },
            "spec": {
                "replicas": 1,
                "selector": {
                    "app": "duraark"
                },
                "template": {
                    "metadata": {
                        "name": "duraark",
                        "labels": {
                            "app": "duraark"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "name": "duraark",
                                "image": "duraark",
                                "ports": [
                                    {
                                        "containerPort": 80
                                    }
                                ]
                            }
                        ]
                    }
                }
            }
        },
        "docker-compose": {
            "duraark-sessions": {
                "build": "./duraark-sessions",
                "volumes": [
                    "/home/martin/01_work/duraark/duraark-storage:/duraark-storage"
                ]
            },
            "duraark-metadata": {
                "build": "./duraark-metadata",
                "volumes_from": [
                    "duraark-sessions"
                ],
                "volumes": [
                    "/var/run/docker.sock:/var/run/docker.sock"
                ],
                "environment": [
                    "DURAARK_STORAGE_PATH=/home/martin/01_work/duraark/duraark-storage"
                ]
            },
            "duraark-sda": {
                "build": "./duraark-sda",
                "volumes": [
                    "/var/run/docker.sock:/var/run/docker.sock"
                ],
                "links": [
                    "duraark-sdas"
                ],
                "environment": [
                    "DURAARK_STORAGE_PATH=/home/martin/01_work/duraark/duraark-storage"
                ]
            },
            "duraark-geometricenrichment": {
                "build": "./duraark-geometricenrichment",
                "volumes_from": [
                    "duraark-sessions"
                ],
                "volumes": [
                    "/var/run/docker.sock:/var/run/docker.sock"
                ],
                "environment": [
                    "DURAARK_STORAGE_PATH=/home/martin/01_work/duraark/duraark-storage"
                ]
            },
            "duraark-digitalpreservation": {
                "build": "./duraark-digitalpreservation",
                "volumes_from": [
                    "duraark-sessions"
                ],
                "links": [
                    "duraark-metadata"
                ],
                "volumes": [
                    "/tmp:/tmp"
                ]
            },
            "duraark-sdas": {
                "image": "eccenca/virtuoso7",
                "ports": [
                    "8890:8890",
                    "1111:1111"
                ],
                "volumes": [
                    "../virtuoso/db:/var/lib/virtuoso/db"
                ]
            },
            "duraark-pointcloud-viewer": {
                "build": "duraark-pointcloud-viewer",
                "ports": [
                    "5016:5016"
                ],
                "volumes_from": [
                    "duraark-sessions"
                ],
                "volumes": [
                    "/var/run/docker.sock:/var/run/docker.sock"
                ]
            },
            "workbench-ui": {
                "build": "./workbench-ui",
                "environment": [
                    "DURAARK_API_ENDPOINT=http://localhost"
                ]
            },
            "web": {
                "build": "./api-gatekeeper",
                "ports": [
                    "80:80"
                ],
                "links": [
                    "duraark-sessions",
                    "duraark-metadata",
                    "duraark-sda",
                    "duraark-geometricenrichment",
                    "duraark-digitalpreservation",
                    "duraark-pointcloud-viewer",
                    "workbench-ui"
                ]
            }
        },
        "service": {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "name": "duraark",
                "labels": {
                    "app": "duraark"
                }
            },
            "spec": {
                "type": "NodePort",
                "ports": [
                    {
                        "port": 80,
                        "targetPort": 80,
                        "protocol": "TCP"
                    }
                ],
                "selector": {
                    "app": "duraark"
                }
            }
        },
        "id": "fa633350-eaba-11e5-aa74-5d34fea124fc",
        "name": "duraark-system",
        "description": ""
    },
