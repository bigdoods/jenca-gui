{
  "name": "jenca-gui",
  "private": true,
  "version": "1.0.0",
  "description": "The frontend code for the Jenca GUI",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenca-cloud/jenca-gui.git"
  },
  "keywords": [
    "jenca",
    "gui"
  ],
  "author": "Jenca",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jenca-cloud/jenca-gui/issues"
  },
  "homepage": "https://github.com/jenca-cloud/jenca-gui#readme",
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist",
    "indexpage": "cp src/index.html dist/index.html",
    "watch": "WEBPACK_ARGS=--watch npm run build",
    "prebuild": "npm run clean && npm run indexpage",
    "build": "NODE_ENV=development webpack $WEBPACK_ARGS",
    "release": "NODE_ENV=production npm run build",
    "serve": "node devServer.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./src/test/setup.js src/test",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "history": "^1.13.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^0.14.1",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^1.0.3",
    "react-router-redux": "^3.0.0",
    "redux": "^3.1.6",
    "redux-logger": "^2.5.2",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "ecstatic": "^1.4.0",
    "express": "^4.13.4",
    "mocha": "^2.4.5",
    "nock": "^7.0.2",
    "react-addons-test-utils": "^0.14.7",
    "redux-mock-store": "0.0.6",
    "webpack": "^1.12.12"
  }
}
